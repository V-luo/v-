指针，数据结构基础
class Solution {
public:
    ListNode* ReverseList(ListNode* pHead) {
        ListNode* newlist = NULL,*p;
        p = pHead;
        while(p)
        {
            ListNode* tmp = p->next;
            p -> next = newlist;
            newlist = p;
            p = tmp;
        }
        return newlist;
    }
};
