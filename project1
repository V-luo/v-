#include <bits/stdc++.h>
using namespace std;
int gcd(int a, int b)
{
    int x, y;
    int r;
    x = a, y = b;
    while (y)
    {
        r = x % y;
        x = y;
        y = r;
    }
    return x;
}
int main()
{
    int a, b;
    cin >> a;
    cin >> b;
    int res;
    res = gcd(a, b);
    cout << res;
}
